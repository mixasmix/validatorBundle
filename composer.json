{
  "name": "mixasmix/validation-bundle",
  "type": "symfony-bundle",
  "description": "Symfony Bundle for ValidationBundle",
  "keywords": ["symfony5", "jwt"],
  "homepage": "https://github.com/mixasmix/validatorBundle",
  "license": "proprietary",
  "authors": [
    {
      "name": "Michael",
      "email": "mixasmix@mail.ru"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "symfony/framework-bundle": "6.0.7",
    "symfony/monolog-bundle": "^3.5",
    "symfony/validator": "6.0.7",
    "symfony/yaml": "6.0.3",
    "ext-bcmath": "*"
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Mixasmix\\ValidationBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "conflict": {
    "symfony/symfony": "<5.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
